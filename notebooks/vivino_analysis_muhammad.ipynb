    "## join 'grapes' table and 'most_used_grapes_per_country' table to convert grape_id in most_used_grapes_per_country to grape_name\n",
    "query_convert_grape_id = query_convert_grape_id = \"\"\"\n",
    "    SELECT most_used_grapes_per_country.country_code, \n",
    "        most_used_grapes_per_country.grape_id, \n",
    "        most_used_grapes_per_country.wines_count,\n",
    "        grapes.id AS grape_id,\n",
    "        grapes.name AS grape_name,\n",
    "        countries.code AS country_code,\n",
    "        countries.name AS country_name\n",
    "    FROM most_used_grapes_per_country\n",
    "    JOIN grapes ON most_used_grapes_per_country.grape_id = grapes.id\n",
    "    JOIN countries ON most_used_grapes_per_country.country_code = countries.code\n",
    "    WHERE most_used_grapes_per_country.wines_count > 1000\n",
    "    ORDER BY most_used_grapes_per_country.wines_count DESC;\n",
    "\"\"\"\n",
    "result_convert_grape_id = session.execute(text(query_convert_grape_id)).fetchall()\n",
    "\n",
    "## convert result to dataframe\n",
    "df_convert_grape_id = pd.DataFrame(result_convert_grape_id, columns=['country_code', 'grape_id', 'wines_count', 'grape_id', 'grape_name', 'country_code', 'country_name'])\n",
    "df_convert_grape_id = df_convert_grape_id.drop(columns=['grape_id', 'country_code'])\n",
    "df_convert_grape_id = df_convert_grape_id.drop_duplicates()\n",
    "df_convert_grape_id = df_convert_grape_id.reset_index(drop=True)\n",
    "\n",
    "## count grape_name in df_convert_grape_id\n",
    "grapename_counter = {}\n",
    "for grape in df_convert_grape_id['grape_name']:\n",
    "    if grape in grapename_counter:\n",
    "        grapename_counter[grape] += 1\n",
    "    else:\n",
    "        grapename_counter[grape] = 1\n",
    "\n",
    "## display df_grapename_counter\n",
    "df_grapename_counter = pd.DataFrame.from_dict(grapename_counter, orient='index', columns=['count'])\n",
    "display(df_grapename_counter)\n",
    "\n",
    "## filter df_convert_grape_id to only include grape_name that appears more than 1 time and country_name that indicates countries in Europe\n",
    "df_convert_grape_id = df_convert_grape_id[df_convert_grape_id['grape_name'].isin(df_grapename_counter[df_grapename_counter['count'] > 1].index)]\n",
    "df_convert_grape_id = df_convert_grape_id[df_convert_grape_id['country_name'].isin(['France', 'Italie', 'Espagne', 'Portugal', 'Allemagne', 'Austria', 'Grèce', 'Hongrie', 'Croatie', 'Georgia', 'Roumanie', 'Slovenia', 'Bulgaria', 'Moldavie', 'England', 'Suisse', 'Turkey', 'Czech Republic', 'Slovakia', 'Luxembourg', 'Serbia', 'Montenegro', 'Macedonia', 'Bosnia and Herzegovina', 'Albania', 'Cyprus', 'Malta', 'Iceland', 'Andorra', 'Liechtenstein', 'Monaco', 'San Marino', 'Vatican City'])]\n",
    "\n",
    "## display df_convert_grape_id\n",
    "display(df_convert_grape_id)\n",
    "\n",
    "\n",
    "## checking tables in vivino.db\n",
    "table_names = engine.table_names()\n",
    "print(table_names)\n",
    "\n"
   ]
