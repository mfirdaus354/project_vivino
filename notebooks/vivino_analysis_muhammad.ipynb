   "source": [
    "## query wines table to convert region_id and winery_id to its associated name\n",
    "\n",
    "query_one = \"\"\"\n",
    "SELECT wines.id, wines.name, wines.ratings_average, wines.ratings_count,\n",
    "    regions.id AS region_id, regions.name AS region, \n",
    "    wineries.id AS winery_id, wineries.name AS winery\n",
    "FROM wines\n",
    "JOIN regions ON wines.region_id = regions.id\n",
    "JOIN wineries ON wines.winery_id = wineries.id;\n",
    "\"\"\"\n",
    "wines_df = pd.read_sql_query(query_one, engine)\n",
    "\n",
    "display(wines_df)\n",
    "\n",
    "\n",
    "\n",
    "## count number of wines per region\n",
    "num_wine_region = wines_df['region'].value_counts(ascending=False)\n",
    "## count number of wines per winery\n",
    "num_wine_winery = wines_df['winery'].value_counts(ascending=False)\n",
    "\n",
    "## count the number of mentioned regions and wineries ini wines_df\n",
    "num_regions = wines_df['region'].nunique()\n",
    "num_wineries = wines_df['winery'].nunique()\n",
    "\n",
    "display(num_wine_region, num_wine_winery, num_regions, num_wineries)\n",
    "\n",
    "\n"
   ]
