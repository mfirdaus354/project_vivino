   "source": [
    "## query 'wines' table to convert its region_id field into region_name by joining with regions table and countries table\n",
    "query_convert_region_id = \"\"\"\n",
    "SELECT wines.id AS wine_id,\n",
    "    wines.name AS wine_name,\n",
    "    wines.region_id AS wine_region_id,\n",
    "    wines.ratings_average AS wine_ratings_average,\n",
    "    wines.ratings_count AS wine_ratings_count,\n",
    "    regions.id AS region_id,\n",
    "    regions.country_code AS region_country_code,\n",
    "    regions.name AS region_name,\n",
    "    countries.code AS country_code,\n",
    "    countries.name AS country_name\n",
    "FROM wines, regions, countries\n",
    "WHERE wine_region_id = regions.id\n",
    "    AND region_country_code = countries.code\n",
    "    AND wine_ratings_average >= 4.0\n",
    "    AND wine_ratings_count > 1000\n",
    "ORDER BY wine_ratings_average DESC;\n",
    "\"\"\"\n",
    "result_convert_region_id = session.execute(text(query_convert_region_id)).fetchall()\n",
    "\n",
    "## convert result to dataframe\n",
    "df_convert_region_id = pd.DataFrame(result_convert_region_id, columns=['wine_id', 'wine_name', 'wine_region_id', 'wine_ratings_average', 'wine_ratings_count', 'region_id', 'region_country_code', 'region_name', 'country_code', 'country_name'])\n",
    "df_convert_region_id = df_convert_region_id.drop(columns=['wine_region_id', 'region_id', 'region_country_code', 'country_code'])\n",
    "df_convert_region_id = df_convert_region_id.drop_duplicates()\n",
    "df_convert_region_id = df_convert_region_id.reset_index(drop=True)\n",
    "\n",
    "## display df_convert_region_id\n",
    "display(df_convert_region_id)"
   ]
